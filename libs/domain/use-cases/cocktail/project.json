{
  "name": "domain-use-cases-cocktail",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/domain/use-cases/cocktail/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/domain/use-cases/cocktail",
        "main": "libs/domain/use-cases/cocktail/src/index.ts",
        "tsConfig": "libs/domain/use-cases/cocktail/tsconfig.lib.json",
        "assets": ["libs/domain/use-cases/cocktail/*.md"],
        "generatePackageJson": true,
        "format": ["cjs"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/domain/use-cases/cocktail/jest.config.ts"
      }
    }
  }
}
